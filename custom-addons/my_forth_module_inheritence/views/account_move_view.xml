<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="account_move_form_my_forth_module_inheritence" model="ir.ui.view">
    <field name="name">account.move.form.my.forth.module.inheritence</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">
      
      <xpath expr="//field[@name='partner_id']" position="after">
        <field name="so_confirm_user_id" />
      </xpath>
      <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='product_id']" position="before">
        <field name="line_number" />
      </xpath>
      <xpath expr="//field[@name='line_ids']/tree/field[@name='account_id']" position="before">
        <field name="line_number" invisible="1"/>
      </xpath>

    </field>
  </record>

</odoo>
